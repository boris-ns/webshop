package webshop.rules;

import rs.ac.uns.ftn.webshopservice.model.Item;


rule "Classify Item - Low Range"
    when
        $i: Item( cost <= 300, category == Category.NA )
    then
        $i.setCategory(Item.Category.LOW_RANGE);
        System.out.println("Aktiviralo se pravilo");
        update($i);
end